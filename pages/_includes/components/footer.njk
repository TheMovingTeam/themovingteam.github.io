<footer>
    <div id="footerWrap">
        <div class="shape">
            <img src="/media/CookieShape.svg" alt="">
        </div>
        <div class="hc">
            <div class="vc">
                <p class="logo">Move</p>
                {% for entry in collections.destination | reverse %}
                {%- if entry.data.title != title %}
                <a class="entry" href="{{entry.url}}">{{ entry.data.title }}</a>
                {%- endif %}
                {% endfor %}
                <a href="https://twitter.com/movetransit">Twitter</a>
            </div>
            <div class="vc jsb">
                <p>Made with ❤️ with 11ty</p>
                <span id="copyright">© FatDawlf, 2025</span>
            </div>
        </div>
    </div>
</footer>
